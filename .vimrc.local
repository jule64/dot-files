" GUI Settings {
    " GVIM- (here instead of .gvimrc)
    if has('gui_running')
        set guifont=Anonymous\ Pro\ Regular:h15
        nmap ,t <Esc>:tabnew<CR>

        if has('gui_macvim')
            set transparency=10          " Make the window slightly transparent
        endif

    endif
" }



